---
permalink: "/feeds/dev.xml"
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0">
  <channel>
    <title>{{ site.title }} - Dev</title>
    <link>{{ site.url }}/dev/</link>
    <description>Dev posts</description>
    {% for post in collections.dev %}
    <item>
      <title>{{ post.data.title }}</title>
      <link>{{ site.url }}{{ post.url }}</link>
      <pubDate>{{ post.date.toUTCString() }}</pubDate>
      <guid>{{ site.url }}{{ post.url }}</guid>
    </item>
    {% endfor %}
  </channel>
</rss>
